{
  "manifest_version": 3,
  "name": "YouTube Transcript Extractor",
  "version": "1.0.0",
  "description": "Extract transcripts from YouTube videos with one click",
  "permissions": [
    "activeTab",
    "scripting",
    "storage"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "host_permissions": [
    "https://www.youtube.com/*"
  ]
}
